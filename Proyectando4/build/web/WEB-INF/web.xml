<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AciertaControlador</servlet-name>
        <servlet-class>AciertaControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conection</servlet-name>
        <servlet-class>Modelo.conection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroDAO</servlet-name>
        <servlet-class>Controlador.RegistroDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UltimasNoticias</servlet-name>
        <servlet-class>Controlador.UltimasNoticias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenerarPost</servlet-name>
        <servlet-class>Controlador.GenerarPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>Controlador.LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarArticulo</servlet-name>
        <servlet-class>Controlador.AgregarArticulo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarArticulo</servlet-name>
        <servlet-class>Controlador.EliminarArticulo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AciertaControlador</servlet-name>
        <url-pattern>/AciertaControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conection</servlet-name>
        <url-pattern>/conection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroDAO</servlet-name>
        <url-pattern>/RegistroDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UltimasNoticias</servlet-name>
        <url-pattern>/UltimasNoticias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerarPost</servlet-name>
        <url-pattern>/GenerarPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarArticulo</servlet-name>
        <url-pattern>/AgregarArticulo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarArticulo</servlet-name>
        <url-pattern>/EliminarArticulo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
